(**
 ** file:	 parse.rml
 ** module:      Parser
 ** description: Interface to external code for parsing
 **
 ** The parser module is used for both parsing of files and statements in
 ** interactive mode. Some relations never fails, even if parsing fails. Instead,
 ** they return an error message other than "Ok".
 **
 ** Input: String to parse
 ** Output: Absyn.Program or InteractiveStmts
 **
 ** $Id: parse.rml,v 1.12 2004/09/28 09:14:38 levsa Exp $
 **
 **)


module Parser:
  with "absyn.rml"
  with "interactive.rml"							
  relation parse: string => Absyn.Program
  relation parseexp: string => Interactive.InteractiveStmts
  relation parsestring: string => (Absyn.Program, string) (* Always succeeds, string contains "Ok" or error message *)
  relation parsestringexp: string => (Interactive.InteractiveStmts, string)  (* Always succeeds, string contains "Ok" or error message *)
end
