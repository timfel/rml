fun makeIDENT(pos1, str) =
  let val pos2 = pos1 + String.size str - 1
      fun loop [] = Tokens.IDENT(str, pos1, pos2)
	| loop((str',mktok)::rest) =
	    if str=str' then mktok(pos1, pos2) else loop rest
  in
    loop kwds
  end

