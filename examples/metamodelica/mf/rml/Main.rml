(* main.rml -- Mini-Freja test driver *)
module Main:
    relation main: string list => ()
end

with "Absyn.rml"
with "Eval.rml"
with "Test.rml"

relation main =
	axiom	main []

	rule	string_int arg => k &
		Test.test k => prog &
		Eval.exec prog &
		main argv
		----------------
		main arg::argv
end
