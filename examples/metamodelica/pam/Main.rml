module Main:

  relation main: () => ()

end

with "Parse.rml"
with "Pam.rml"

(* Parse and translate a PAM program into MCode,
 * then emit it as textual assembly code.
 *)
relation main: () => ()  =

  rule  Parse.parse() => program &
        Pam.eval_stmt([], program) => _
        --------------------
        main

end (* main *)

